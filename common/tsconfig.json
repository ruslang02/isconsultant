{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "Node",
    "lib": [
      "es6"
    ],
    "declaration": true,
    "declarationMap": false,
    "noResolve": false,
    "rootDir": ".",
    "outDir": "../dist",
    "composite": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "experimentalDecorators": true,
    "tsBuildInfoFile": "../dist/common.tsbuildinfo"
  },
  "references": []
}